<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/container"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".ui.hmsservice.damagereport.FragmentDamReportMain">

    <TextView
        android:id="@+id/damrep_main_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="80dp"
        android:text="Skaderapport"
        />

    <TextView
        android:id="@+id/damrep_main_desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="*sett inn velkomstmelding med instrukser*"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/damrep_main_id"
        />

    <Button
        android:id="@+id/damrep_main_nav_addbooking"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/damrep_main_desc"
        android:text="@string/dr_m_nav_add"/>

    <Button
        android:id="@+id/damrep_main_nav_rembooking"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/damrep_main_nav_addbooking"
        android:text="@string/dr_m_nav_rem"/>

    <Button
        android:id="@+id/damrep_main_nav_updbooking"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/damrep_main_nav_rembooking"
        android:text="@string/dr_m_nav_upd"/>

    <Button
        android:id="@+id/damrep_main_nav_getreports"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/damrep_main_nav_updbooking"
        android:text="@string/dr_m_nav_get"/>



</androidx.constraintlayout.widget.ConstraintLayout>